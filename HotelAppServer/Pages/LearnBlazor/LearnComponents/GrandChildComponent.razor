<div class="row">
    <h4 class="text-primary pl-4 pt-2 col-12">Grand Child Component</h4>
    <br />
    <p class="text-primary pl-5"> There is a message - @MessageForGrandChild </p>
    <p class="text-primary pl-5"> Grand Parent Age - @GrandParentAge </p>

</div>

@code {
    [CascadingParameter(Name = "message")]
    public string MessageForGrandChild { get; set; }
    [CascadingParameter(Name = "age")]
    public string GrandParentAge { get; set; }
}
